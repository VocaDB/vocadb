@using VocaDb.Web.Code
@using VocaDb.Web.Models.Shared.Partials.ArchivedEntry
@model VocaDb.Web.Models.Tag.Versions

@{
	PageProperties.Title = ViewRes.EntryDetailsStrings.Revisions + " - " + Model.Entry.Name;
	PageProperties.Robots = PagePropertiesData.Robots_Noindex_Nofollow;
	ViewBag.Parents = new[] { 
		Html.ActionLink(ViewRes.SharedStrings.Tags, "Index"), 
		Html.ActionLink(Model.Entry.Name, "DetailsById", new { id = Model.Entry.Id, slug = Model.Entry.UrlSlug }) 
	};
}

<partial name="Partials/ArchivedEntry/_CurrentVersionMessage" model="@(new CurrentVersionMessageViewModel(Model.Entry.Version, Model.Entry.Status))" />

<partial name="Partials/ArchivedEntry/_ArchivedObjectVersions" model="@(new ArchivedObjectVersionsViewModel(Model.ArchivedVersions, id => Url.Action("ViewVersion", new { id })))" />